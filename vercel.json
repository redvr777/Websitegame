
{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "redirects": [
    {
      "source": "/",
      "destination": "/home",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/home",
      "destination": "/index.html"
    },
    {
      "source": "/story",
      "destination": "/story.html"
    },
    {
      "source": "/characters",
      "destination": "/characters.html"
    },
    {
      "source": "/world",
      "destination": "/world.html"
    },

    {
      "source": "/upcoming-content",
      "destination": "/upcoming.html"
    },
    {
      "source": "/settings",
      "destination": "/settings.html"
    },
    {
      "source": "/settings/privacy-policy",
      "destination": "/policy.html"
    },
    {
      "source": "/settings/terms-and-conditions",
      "destination": "/terms.html"
    },
    {
      "source": "/settings/rules",
      "destination": "/rules.html"
    }

  ]
}
